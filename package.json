{
  "name": "tastyspore",
  "version": "1.0.0",
  "private": true,
  "description": "tastyspore",
  "author": "Trevor Smith",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "gatsby develop -H 0.0.0.0",
    "cname:add": "echo tastyspore.com > public/CNAME",
    "build": "yarn clean && gatsby build && yarn cname:add",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "publish": "gh-pages -d public",
    "cap:update": "npx cap update android",
    "cap:open": "npx cap open android",
    "android": "yarn cap:update && yarn cap:open"
  },
  "dependencies": {
    "@capacitor/android": "^3.3.4",
    "@capacitor/cli": "^3.3.4",
    "@capacitor/core": "^3.3.4",
    "@capacitor/ios": "^3.3.4",
    "@formspree/react": "^2.2.4",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/gatsby-plugin-breakpoints": "^1.3.2",
    "gatsby": "^4.4.0",
    "gatsby-plugin-breakpoints": "^1.3.6",
    "gatsby-plugin-google-analytics": "^4.4.0",
    "gatsby-plugin-google-tagmanager": "^4.4.0",
    "gatsby-plugin-graphql-codegen": "^3.1.0",
    "gatsby-plugin-htaccess": "^1.4.0",
    "gatsby-plugin-image": "^2.4.0",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-mdx": "^3.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sass": "^5.4.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-plugin-sitemap": "^5.4.0",
    "gatsby-plugin-ts": "^3.1.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "intersection-observer": "^0.12.0",
    "minireset.css": "^0.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.3.3",
    "sass": "^1.45.1",
    "usehooks-ts": "^2.1.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.5"
  }
}
